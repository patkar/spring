<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id="collectionexample" class="com.deloitte.collectionexample">
   	<property name="cityList">
   		<list>
   			<value>Bangalore</value>
   			<value>Hydrabad</value>
   			<value>chennai</value>
   			<value>mumbai</value>
   		</list>
   	</property>
   	<property name="citySetl">
   	<set>
   			<value>Bangalore</value>
   			<value>Hydrabad</value>
   			<value>chennai</value>
   			<value>mumbai</value>
   	</set>
   	</property>
   	<property name="cityMap">
   	<map>
   			<entry key="1" value="bangalore"/>
   			<entry key="2" value="hydrabad"/>
   			<entry key="3" value="chennai"/>
   			<entry key="4" value="mumbai"/>
   	</map>
   	</property>
   	<property name="cityProp">
   		<props>
   			<prop key="one">Bangalore</prop>
   			<prop key="two">Hydrabad</prop>
   			<prop key="three">chennai</prop>
   			<prop key="four">mumbai</prop>
   		</props>
   	</property>
   </bean>
</beans>   